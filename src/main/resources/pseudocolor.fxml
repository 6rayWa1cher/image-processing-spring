                    <children>
                        <Spinner fx:id="q1chooser" editable="true" layoutY="25.0" prefHeight="17.0" prefWidth="113.0"
                                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                        </Spinner>
                        <ColorPicker fx:id="picker1" layoutX="4.0" layoutY="6.0" onAction="#onChange"
                                     AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"

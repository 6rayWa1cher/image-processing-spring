                    <children>
                        <Spinner fx:id="q3chooser" editable="true" layoutY="25.0" prefHeight="17.0" prefWidth="113.0"
                                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                        </Spinner>
                        <ColorPicker fx:id="picker3" layoutX="4.0" layoutY="6.0" onAction="#onChange"
